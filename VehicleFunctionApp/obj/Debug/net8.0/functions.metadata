[
  {
    "name": "GetVehicle",
    "scriptFile": "VehicleFunctionApp.dll",
    "entryPoint": "VehicleFunctionApp.Functions.GetVehicleFunction.Run",
    "language": "dotnet-isolated",
    "properties": {
      "IsCodeless": false
    },
    "bindings": [
      {
        "name": "req",
        "direction": "In",
        "type": "httpTrigger",
        "authLevel": "Anonymous",
        "methods": [
          "get"
        ],
        "route": "vehicle/{registrationNumber}",
        "properties": {}
      },
      {
        "name": "$return",
        "type": "http",
        "direction": "Out"
      }
    ]
  }
]